{
  "name": "egg-consul-client",
  "version": "1.0.0",
  "description": "Consul-Client plugin for Egg",
  "eggPlugin": {
    "name": "consul"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-plugin-consul",
    "egg-plugin-consul-client"
  ],
  "dependencies": {
    "consul": "^0.40.0"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.29.4",
    "egg-bin": "^4.16.2",
    "egg-ci": "^1.18.0",
    "egg-mock": "^4.1.0",
    "eslint": "^7.29.0",
    "eslint-config-egg": "^9.0.0"
  },
  "engines": {
