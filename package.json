{
  "name": "egg-consul-client",
  "version": "1.3.1",
  "description": "Consul-Client plugin for Egg",
  "eggPlugin": {
    "name": "consul"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "egg-plugin-consul",
    "egg-plugin-consul-client"
  ],
  "dependencies": {
    "@sindresorhus/is": "^4.0.1",
    "consul": "^0.40.0"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.29.4",
